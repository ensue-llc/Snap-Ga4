{
    "name": "ensue/ga4",
    "description": "Reporting API for Google analytics V4",
    "keywords": [
        "ensue",
        "analytics"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Ensue\\GA4\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Bipin Maharjan",
            "email": "bipin.maharjan@ensue.us"
        }
    ],
    "require": {
        "php": "^8.2",
        "google/analytics-data": "^0.22",
        "laravel/framework": "^12.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ensue\\GA4\\GA4ServiceProvider"
            ],
            "aliases": {
                "GA4": "Ensue\\GA4\\Facades\\GA4"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
